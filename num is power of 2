package com.psna.training;
import java.util.Scanner;
public class Ece {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		if((n & (n-1)) == 0)
			System.out.println(n+" is power of 2");
		else
			System.out.println(n+" is not a power of 2");
	
	
	}	
	
}
	
