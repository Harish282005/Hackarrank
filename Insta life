import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner scan = new Scanner(System.in);
        String name = scan.next();
        int insta = scan.nextInt();
        int post = scan.nextInt();
        float hour = scan.nextFloat();
        System.out.println("My name is "+name +", my Instagram has " +insta +" followers, I have posted " +post+
                           " posts, and I use Instagram for " +hour+" hours daily.");
        
    }
}
